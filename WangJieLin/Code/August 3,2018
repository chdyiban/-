<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8" />
		<title></title>
	</head>
	<body>
		<script>
            function p(s){
                document.write(s);
                document.write("<br>");
            }
            function check(value,a){
               for(i in a){
                 if(value==a[i])
                 return true;
               }
               return false;
            }

            function removeDuplicate(a){
            var result =new Array();
            while(a.length!=0){
               var v = a.pop();
               console.log(v);
               if( !check(v,a) ){
                 result.push(v);
                }
            }
            return result;
            }
            
            var x = new Array(1,3,4,5,7,7,4,5,6,7,7); 
            p('数组x是:'+x);
            x = removeDuplicate(x);
            x.sort();
            p('使用sort排序后的无重复数据的数组x是:'+x);
        </script>
	</body>
</html>
